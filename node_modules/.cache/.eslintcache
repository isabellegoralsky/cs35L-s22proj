\n  )\n}\n\nclass Board extends React.Component {\n  constructor(props) {\n    super(props);  \n    let value_range = [];\n    for (var i = 1; i <= 8; i++) {\n      value_range.push(i);\n    }\n    let vr = value_range.concat(value_range);\n    let vrs = shuffle(vr);\n    let pset;\n    this.state = {   \n      img_show: Array(16).fill(0),\n      img_matched: Array(16).fill(0),\n      img_value: vrs,\n      first_pick: true,\n      first_pick_index: -1,\n      score: 0,\n      playable: 1,\n      pset: \"bull\",\n    };\n  }\n\n  handleChange(event){\n    console.log(\"event: \", event);\n    console.log(\"event.target.value: \", event.target.value);\n    console.log(\"event.target.id: \", event.target.id);\n    this.setState({\n      pset: event.target.value,\n    });\n  }\n\n  handleClick(i) {\n    console.log(\"clicked i: \", i)\n    const img_show = this.state.img_show.slice();\n    const img_matched = this.state.img_matched.slice();\n    const img_value = this.state.img_value.slice();\n    if (this.state.playable==0 || calculateWinner(img_matched) || img_matched[i]){\n      return;\n    }\n    if (img_show.includes(img_value[i]) && i!=this.state.first_pick_index){\n      img_matched[i]=1;\n      img_matched[this.state.first_pick_index]=1;\n      img_show[i] = img_value[i];\n      this.setState({\n        img_show: img_show,\n        img_matched: img_matched,\n        first_pick: true,\n        first_pick_index: -1,\n        score: this.state.score + 1,\n      });\n    }else{\n      if (this.state.first_pick==true){\n        img_show[i] = img_value[i];\n        this.setState({\n          img_show: img_show,\n          first_pick: false,\n          first_pick_index: i,\n          score: this.state.score + 1,\n        });\n      }else{\n        img_show[i] = img_value[i];\n        this.setState({\n          playable: 0,\n          img_show: img_show,\n          score: this.state.score + 1,\n        });\n        setTimeout(function(){\n          img_show[this.state.first_pick_index] = 0;\n          img_show[i] = 0; \n          this.setState({\n            playable: 1, \n            img_show: img_show,\n            first_pick: true,\n            first_pick_index: -1,\n          });\n        }.bind(this), 1500);\n      }\n    }\n  }\n\n  renderSquare(i) {\n    return (\n      <Square\n        value={this.state.img_show[i]}\n        onClick={() => this.handleClick(i)}\n      />\n    );\n  }\n      \n  renderImage(i) {\n    var index = this.state.img_show[i]\n    console.log(\"pset: \", this.state.pset);\n    if (this.state.pset == \"bull\"){\n      console.log(\"Bulldogs!\");\n      var imgname = \"bulldog/\" + this.state.pset + index + \".jpg\";\n    }\n    else if (this.state.pset == \"chicks\"){\n      console.log(\"Lil Baby Chicks!\");\n      var imgname = \"chickens/\" + this.state.pset + index + \".jpg\";\n    }\n    else if (this.state.pset == \"p\"){\n      console.log(\"Pups!\");\n      var imgname = \"puppies/\" + this.state.pset + index + \".jpg\";\n    }\n    else if (this.state.pset == \"s\"){\n      console.log(\"Under the Sea!\");\n      var imgname = \"sea/\" + this.state.pset + index + \".jpg\";\n    }\n    console.log(\"Asked for image: \", imgname);\n    return (\n      <Image      \n        source={imgname}\n\tonClick={() => this.handleClick(i)}\n       />\n    );\n  }\n\n  render() {\n    const winner = calculateWinner(this.state.img_matched);\n    let status;\n    let imgarg;\n    if (winner) {\n      status = 'Winner! Score: ' + this.state.score;\n    } else{\n      status = 'Choose a Card. Score: ' + this.state.score;\n    }\n      \n    var ar = [0,1,2,3];\n    return (\n      <div>\n        <label>\n          Choose your photoset:\n          <select id = \"pset\" value={this.state.pset} onChange={(e) => {this.handleChange(e)}}>\n            <option value=\"bull\">Bulldogs</option>\n            <option value=\"p\">Puppies</option>\n            <option value=\"chicks\">BabyChickens</option>\n            <option value=\"s\">SeaLife</option>\n          </select>\n        </label>\n      <div className=\"status\">{status}</div>\n      <div className=\"rows\">\n        <div className=\"img-row\">\n          <div\nclassName=\"lib-img\">{this.renderImage(0)}</div>\n         <div\nclassName=\"lib-img\">{this.renderImage(1)}</div>\n          <div\nclassName=\"lib-img\">{this.renderImage(2)}</div>\n          <div\nclassName=\"lib-img\">{this.renderImage(3)}</div>\n        </div>\n<div className=\"img-row\">\n          <div\nclassName=\"lib-img\">{this.renderImage(4)}</div>\n          <div\nclassName=\"lib-img\">{this.renderImage(5)}</div>\n          <div\nclassName=\"lib-img\">{this.renderImage(6)}</div>\n          <div\nclassName=\"lib-img\">{this.renderImage(7)}</div>\n        </div>\n<div className=\"img-row\">\n          <div\nclassName=\"lib-img\">{this.renderImage(8)}</div>\n          <div\nclassName=\"lib-img\">{this.renderImage(9)}</div>\n          <div\nclassName=\"lib-img\">{this.renderImage(10)}</div>\n          <div\nclassName=\"lib-img\">{this.renderImage(11)}</div>\n        </div>\n<div className=\"img-row\">\n          <div\nclassName=\"lib-img\">{this.renderImage(12)}</div>\n          <div\nclassName=\"lib-img\">{this.renderImage(13)}</div>\n          <div\nclassName=\"lib-img\">{this.renderImage(14)}</div>\n          <div\nclassName=\"lib-img\">{this.renderImage(15)}</div>\n        </div>\n      </div>\n    </div>\n    );    \n  }\n}\n\n  \nclass Game extends React.Component {\n  render() {   \n    return (\n      <div className=\"game\">\n        <div className=\"game-board\">\n          <Board />\n        </div>\n        <div className=\"game-info\">\n          <div>{/* status */}</div>\n          <ol>{/* TODO */}</ol>\n        </div>\n      </div>\n    );\n  }\n}\n\n// ========================================\n\nconst root = ReactDOM.createRoot(document.getElementById(\"root\"));\nroot.render(<Game />);\n\nfunction calculateWinner(img_matched) {\n  let win = 1;\n  for (let i = 0; i < img_matched.length; i++) {\n    if (img_matched[i]==0){   \n      win = img_matched[i];\n    }\n  }\n  return win;\n}      \n",{"ruleId":"73","severity":1,"message":"74","line":51,"column":28,"nodeType":"75","messageId":"76","endLine":51,"endColumn":30},{"ruleId":"73","severity":1,"message":"77","line":55,"column":51,"nodeType":"75","messageId":"76","endLine":55,"endColumn":53},{"ruleId":"73","severity":1,"message":"74","line":67,"column":32,"nodeType":"75","messageId":"76","endLine":67,"endColumn":34},{"ruleId":"78","severity":1,"message":"79","line":39,"column":18,"nodeType":"80","messageId":"81","endLine":39,"endColumn":27,"fix":"82"},{"ruleId":"73","severity":1,"message":"74","line":77,"column":28,"nodeType":"75","messageId":"76","endLine":77,"endColumn":30},{"ruleId":"73","severity":1,"message":"77","line":81,"column":51,"nodeType":"75","messageId":"76","endLine":81,"endColumn":53},{"ruleId":"73","severity":1,"message":"74","line":93,"column":32,"nodeType":"75","messageId":"76","endLine":93,"endColumn":34},{"ruleId":"83","severity":1,"message":"84","line":5,"column":10,"nodeType":"85","messageId":"86","endLine":5,"endColumn":20},{"ruleId":"78","severity":1,"message":"79","line":12,"column":21,"nodeType":"80","messageId":"81","endLine":12,"endColumn":30,"fix":"87"},{"ruleId":"83","severity":1,"message":"88","line":24,"column":7,"nodeType":"85","messageId":"86","endLine":24,"endColumn":17},{"ruleId":"73","severity":1,"message":"77","line":28,"column":23,"nodeType":"75","messageId":"76","endLine":28,"endColumn":25},{"ruleId":"83","severity":1,"message":"89","line":85,"column":9,"nodeType":"85","messageId":"86","endLine":85,"endColumn":13},{"ruleId":"73","severity":1,"message":"74","line":112,"column":28,"nodeType":"75","messageId":"76","endLine":112,"endColumn":30},{"ruleId":"73","severity":1,"message":"77","line":115,"column":45,"nodeType":"75","messageId":"76","endLine":115,"endColumn":47},{"ruleId":"73","severity":1,"message":"74",